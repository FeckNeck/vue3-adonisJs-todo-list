#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/feck/dev/vue-adonisjs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/feck/dev/vue-adonisjs/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../pino-pretty@9.4.0/node_modules/pino-pretty/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../pino-pretty@9.4.0/node_modules/pino-pretty/bin.js" "$@"
fi
